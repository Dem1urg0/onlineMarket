{#
    Шаблон страницы "Заказы" для администратора
    Выводит список заказов
    Компоненты:
        order-page - компонент заказов
    параметры:
        orders - список заказов
        currentUser - текущий пользователь
#}
{% extends '/admin/layouts/main.twig' %}
{% block content %}
    {# Подключение компонента заказов #}
    <order-page :orders="{{ orders | json_encode }}" :current-user="{{ currentUser | json_encode }}"></order-page>
{% endblock %}

